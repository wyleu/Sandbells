{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Test: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "test"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/changes/manage.py"
        },
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/changes/manage.py"
        },
        {
            "name": " Make Migration: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "makemigrations"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/changes/manage.py"
        },
        {
            "name": " Migrate: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "migrate"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/changes/manage.py"
        },
          {
                "name": "Test:ALL",
                "type": "debugpy",
                "request": "launch",
                "program": "${workspaceFolder}/changes/manage.py",
                "args": ["test", "changes"]
            },
            {
                "name": "Test:SPECIFIC",
                "type": "debugpy",
                "request": "launch",
                "program": "${workspaceFolder}/changes/manage.py",
                "args": [
                "test",
                "--keepdb",
                "appname.tests.modulename.ClassName.test_name"
                ],
                "django": true
            },
            {
                "name": "Django Tests",
                "type": "debugpy",
                "request": "launch",
                "program": "${workspaceFolder}/changes/manage.py",
                "args": ["test"],
                "django": true
            },
            {
                "name": "View Test Results",
                "type": "debugpy",
                "request": "launch",
                "program": "${workspaceFolder}/changes/manage.py",
                "args": ["showtestfails"],
                "django": true
            },
    ]
}



